#define LL long long

using namespace std;
int ans[560];
int hs[560];
int n; 
int main(){
     while(scanf("%d",&n) != EOF,n)
	   {
	      for(int i = 1;i <= n;i ++)
		  {
		     for(int j = 1;j <= 2*n ;j ++)
			 {
			  int temp;
			  scanf("%d",&temp);
			   if(j % 2 ==  1)
			   {
			     hs[temp] ++ ;  
			   }
			 } 
		  }
		   
	   
	   }
	 
	return 0;
}
